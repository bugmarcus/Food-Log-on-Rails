<% content_for :title do %>
  Foodlog &ndash; <%= DateTime.now %>
<% end %>

<p style="color: green"><%= notice %></p>

<h1>Entries</h1>

<div id="entries">
  <% @entries.each do |entry| %>
    <%= render entry %>
    <p>
      <%= link_to "Show this entry", entry %>
    </p>
  <% end %>
</div>

<%= link_to "New Entry", new_entry_path %>
